{
    "name": "misantron/dbunit",
    "description": "DbUnit fork supporting PHPUnit 8/9",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "database",
        "testing",
        "dbunit"
    ],
    "authors": [
        {
            "name": "Aleksandr Ivanov",
            "email": "misantron@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-pdo": "*",
        "phpunit/phpunit": "^8.5 || ^9.2",
        "symfony/yaml": "^4.4 || ^5.0 || ^6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.18 || ^3.3",
        "php-coveralls/php-coveralls": "^2.5",
        "phpstan/phpstan": "^1.2",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "PHPUnit\\DbUnit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PHPUnit\\DbUnit\\Tests\\": "tests/"
        },
        "files": [
            "tests/_files/DatabaseTestUtility.php"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
